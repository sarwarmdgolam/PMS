    Font-Size="8pt"     
    ForeColor="#585858"  
    Height="17px"   
    >
</asp:DropDownList>

<asp:Button runat="server" 
    Font-Names="Tahoma" 
    Font-Size="8pt"
    Height="30px"
    Width="100px" 
    BackColor="#F5F5ED" 
    BorderWidth="1px" 
    BorderColor="#426194"
    BorderStyle="Solid" 
    SkinID="insertbutton"
    />



    

<asp:RadioButton runat="server" 
    SkinID="XTabRadioButton"
    Font-Names="Tahoma" 
    Font-Size="8pt"     
    ForeColor="Black">
</asp:RadioButton>



<asp:ListBox runat="server" 
    Font-Names="Tahoma" 
    Font-Size="8pt"     
    BorderStyle="Solid" 
    BorderColor="#6D85C3" 
    ForeColor="Black"  
    Height="102px"
    border="1px"   
    Width="99%">
</asp:ListBox>


<asp:ValidationSummary 
    runat="server"     
    style="width:98%" DisplayMode="BulletList" CssClass="ValidationSummary" 
/>



